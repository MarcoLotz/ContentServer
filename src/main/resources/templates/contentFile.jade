-@ import val item: com.marcolotz.contentserver.filesystem.FileSystemItem
p <strong>File:</strong>
p name: #{name}
p extension type: <italic>#{extension}</italic>
p size: #{humanReadableSize}
p readable: #{isReadable}
<a href="/download/#{item.hashCode.toString}"><input type="button" value="Download file"/></a>
